{
  "name": "The Kim in the North!",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Pledge allegiance to the rightful Kim in the North on every page in WWWesteros!",
  "homepage_url": "https://github.com/WoodenDoors/kiminthenorthinator",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "src/inject.js"
      ],
      "run_at": "document_end"
    }
  ]
}
